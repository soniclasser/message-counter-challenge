{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend for message count challenge",
  "scripts": {
    "start": "ts-node src/api.ts",
    "start:api": "ts-node src/api.ts",
    "start:worker": "ts-node src/worker.ts",
    "migrate": "npx ts-node node_modules/sequelize-cli/lib/sequelize db:migrate --config=./sequelize.config.ts",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migration:create": "npx sequelize-cli migration:create --name",
    "dev": "nodemon --exec ts-node src/api.ts",
    "build": "tsc"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "axios": "^1.7.2",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "tsyringe": "^4.10.0"
  },
  "author": "Carlos Suarez Santiago",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/sequelize": "^4.28.20",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
